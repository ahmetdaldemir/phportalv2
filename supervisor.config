[program:job]
process_name=%(program_name)s_%(process_num)02d
command=cd /var/www/vhosts/phonehospital.com.tr/crm/httpdocs && php artisan  tail:work --sleep=3 --tries=3
autostart=true
autorestart=true
user= root
numprocs=8
redirect_stderr=true
stdout_logfile=var/www/vhosts/phonehospital.com/crm/httpdocs/storage/logs/worker.log
stopwaitsecs=3600
