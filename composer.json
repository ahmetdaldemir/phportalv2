{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.4.0",
        "ext-pcntl": "*",
        "ext-soap": "*",
        "barryvdh/laravel-dompdf": "^2.0",
        "bensampo/laravel-enum": "^6.3",
        "guzzlehttp/guzzle": "^7.2",
        "haruncpi/laravel-log-reader": "^1.0",
        "hasokeyk/elogo": "^1.0",
        "irfa/php-sn-generator": "^1.1",
        "laravel/framework": "^11.0",
        "laravel/horizon": "^5.34",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.5",
        "mcaskill/php-array-group-by": "^1.0",
        "michael-rubel/laravel-value-objects": "^7.0.2",
        "milon/barcode": "^11.0.1",
        "picqer/php-barcode-generator": "^3.2",
        "predis/predis": "^2.1",
        "spatie/array-to-xml": "^3.1",
        "spatie/image-optimizer": "^1.6",
        "spatie/laravel-permission": "^6.0",
        "spatie/simple-excel": "^3.1",
        "torann/currency": "^1.1",
        "yaza/laravel-repository-service": "^6.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/sail": "^1.30",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files": [
            "app/Helper/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
